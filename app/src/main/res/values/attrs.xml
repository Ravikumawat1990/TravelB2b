<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="mtplviews">
        <attr name="TypeFace" format="string" />
        <attr name="Hint" format="string" />
    </declare-styleable>
    <declare-styleable name="headerview">
        <attr name="headerText" format="string" />
        <attr name="headerTextColor" format="color" />
        <attr name="backImgSrc" format="integer" />
        <attr name="showMenu" format="boolean" />
        <attr name="showBack" format="boolean" />
        <attr name="showCalender" format="boolean" />
        <attr name="showShare" format="boolean" />
        <attr name="showCancel" format="boolean" />

    </declare-styleable>
    <!-- Attributes for RoundedImageView -->
    <declare-styleable name="RoundedImageView">
        <!-- Size to round image corners by. Default is zero, unrounded.
        <b>NOTE:</b> This only works for bitmap image sources. -->
        <attr name="corner_radius" format="dimension" />
    </declare-styleable>
</resources>